[run]
source = .
omit = 
    tests/*
    */tests/*
    test_*.py
    *_test.py
    run_*.py
    templates/*
    htmlcov/*
    .git/*
    __pycache__/*
    */venv/*
    */env/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod

[html]
directory = htmlcov
